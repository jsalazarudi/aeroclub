{{ form_start(form,{'attr': {'class': 'p-2' }}) }}
<div class="row">
    <div class="col form-check form-switch">
        {{ form_label(form.socio) }}

        {{ form_widget(form.socio) }}

        {% if form_errors(form.socio) %}
            <div class="alert alert-danger">
                {{ form_errors(form.socio) }}
            </div>
        {% endif %}

    </div>
    <div class="col form-check form-switch">
        {{ form_label(form.alumno) }}

        {{ form_widget(form.alumno) }}

        {% if form_errors(form.alumno) %}
            <div class="alert alert-danger">
                {{ form_errors(form.alumno) }}
            </div>
        {% endif %}

    </div>
    <div class="col form-check form-switch">
        {{ form_label(form.bautismo) }}

        {{ form_widget(form.bautismo) }}

        {% if form_errors(form.bautismo) %}
            <div class="alert alert-danger">
                {{ form_errors(form.bautismo) }}
            </div>
        {% endif %}

    </div>
</div>
