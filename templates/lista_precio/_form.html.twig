        {% endif %}

        {{ form_widget(form.producto) }}
    </div>
</div>
<div class="row">
    <div class="col mb-3">
        {{ form_label(form.precio) }}

        {% if form_errors(form.precio) %}
            <div class="alert alert-danger">
                {{ form_errors(form.precio) }}
            </div>
        {% endif %}

        {{ form_widget(form.precio) }}
