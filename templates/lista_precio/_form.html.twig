                {{ form_errors(form.servicio) }}
            </div>
        {% endif %}

        {{ form_widget(form.servicio) }}
    </div>
    <div class="col mb-3">
        {{ form_label(form.avion) }}

        {% if form_errors(form.avion) %}
            <div class="alert alert-danger">
                {{ form_errors(form.avion) }}
            </div>
        {% endif %}

        {{ form_widget(form.avion) }}
    </div>
</div>
<div class="row">
